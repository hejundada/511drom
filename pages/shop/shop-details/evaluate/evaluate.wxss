.evaluate-content{
  width: 100%;
  min-height: 100vh;
  background-color: #fff;
}
.content-box{
  width: 95%;
  margin: auto;
}
.content-box .content-list{
  width: 100%;
  border-top: 1px solid #eee;
}
.content-box .content-list .list-headline{
  margin: 20rpx 0;
  display: flex;
  justify-content: space-between;
}
.list-headline .line-text{
  font-size: 24rpx;
  color: #999;
}
.content-list .list-eva{
  margin-bottom: 20rpx;
  font-size: 30rpx;
}